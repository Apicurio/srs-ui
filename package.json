{
  "name": "srs-ui",
  "federatedModuleName": "srs",
  "version": "0.0.2",
  "description": "The Service Registry UI for Red Hat Kafka Instances for Apache Kafka",
  "main": "server.js",
  "port": "1337",
  "repository": "https://github.com/bf2fc6cc711aee1a0c2a/srs-ui.git",
  "homepage": "https://cloud.redhat.com/application-services/openshift-streams",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "node server.js",
    "start:mockserver": "asapi",
    "start:dev": "webpack serve --https --hot --color --progress --config webpack.dev.js",
    "start:federate": "HOST=localhost PORT=9005 PROTOCOL=http webpack serve --hot --color --progress --config webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts,js}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 7001",
    "build:storybook": "build-storybook",
    "i18n": "i18next 'src/**/**/*.{ts,tsx}' [-oc] -c './i18next-parser.config.js' -o './src/locales/$LOCALE/$NAMESPACE.json'"
  },
  "devDependencies": {
    "@bf2/ui-shared": "^0.1.2",
    "@redhat-cloud-services/frontend-components-config": "4.1.9",
    "@rhoas/api-mock": "^0.4.0-dev3",
    "@rhoas/registry-management-sdk": "^0.3.2",
    "@storybook/addon-actions": "6.1.14",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.1.14",
    "@storybook/addons": "6.1.14",
    "@storybook/react": "6.2.9",
    "@types/enzyme": "3.10.8",
    "@types/jest": "26.0.20",
    "@types/react-router-dom": "5.1.7",
    "@types/victory": "33.1.4",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.4.1",
    "copy-webpack-plugin": "8.1.1",
    "css-loader": "5.2.4",
    "css-minimizer-webpack-plugin": "2.0.0",
    "dotenv-webpack": "7.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.18.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "imagemin": "7.0.1",
    "jest": "26.6.3",
    "mini-css-extract-plugin": "1.6.0",
    "optimize-css-assets-webpack-plugin": "6.0.0",
    "postcss": "8.3.0",
    "postcss-loader": "5.3.0",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "raw-loader": "4.0.2",
    "react-axe": "3.5.4",
    "react-docgen-typescript-loader": "3.7.2",
    "react-router-dom": "5.2.0",
    "regenerator-runtime": "0.13.7",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "svg-url-loader": "7.1.1",
    "terser-webpack-plugin": "5.1.1",
    "ts-jest": "26.4.4",
    "ts-loader": "9.1.1",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "tslib": "2.1.0",
    "typescript": "4.1.3",
    "url-loader": "4.1.1",
    "webpack": "5.36.2",
    "webpack-bundle-analyzer": "4.4.1",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3"
  },
  "dependencies": {
    "@patternfly/react-core": "4.84.4",
    "@patternfly/react-icons": "4.7.22",
    "@patternfly/react-styles": "4.7.22",
    "@patternfly/react-table": "4.27.7",
    "@testing-library/dom": "7.31.0",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "7.0.0",
    "@testing-library/user-event": "13.1.9",
    "dayjs": "1.10.5",
    "i18next": "20.3.0",
    "i18next-browser-languagedetector": "6.1.1",
    "i18next-http-backend": "1.2.6",
    "i18next-pseudo": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "11.9.0",
    "react-router-last-location": "2.0.1",
    "sirv-cli": "1.0.10"
  }
}
